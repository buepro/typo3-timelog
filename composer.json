{
  "name": "buepro/timelog",
  "type": "typo3-cms-extension",
  "description": "The aim of this extension is to increase the efficiency and transparency of work by continuously collecting work information and communicate it to customers.",
  "keywords": [
    "TYPO3",
    "timelog"
  ],
  "authors": [
    {
      "name": "Roman B\u00fcchler",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/buepro/typo3-timelog/",
  "license": [
    "MIT"
  ],
  "require": {
    "typo3/cms-core": "^9.5",
    "buepro/typo3-auxlibs": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Buepro\\Timelog\\": "Classes"
    }
  },
  "replace": {
    "timelog": "self.version",
    "typo3-ter/timelog": "self.version"
  }
}
